{
  "name": "@headless-charts/react",
  "version": "0.2.1",
  "description": "React wrapper for headless-charts",
  "author": "Vonnue dev team",
  "keywords": [
    "react",
    "headless-charts",
    "charts"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hacklehub/headless-charts.git"
  },
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "start": "tsc -w",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --watch --reporter verbose --ui --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "publish": "vite build && npm publish"
  },
  "dependencies": {
    "d3": "^7.9.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-links": "^10.0.6",
    "@storybook/addon-themes": "^10.0.6",
    "@storybook/react": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.3",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-istanbul": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^10.0.6",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^10.0.6",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  }
}
